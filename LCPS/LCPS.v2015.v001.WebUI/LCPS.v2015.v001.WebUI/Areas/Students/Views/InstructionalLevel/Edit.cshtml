@model LCPS.v2015.v001.NwUsers.Students.InstructionalLevel

@{
    ViewBag.Title = "Edit";
    Layout = "~/Areas/Students/Views/Shared/_StudentLayout.cshtml";
}

<h2>Edit</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>InstructionalLevel</h4>
        <hr />
        @Html.ValidationSummary(true)
        @Html.HiddenFor(model => model.InstructionalLevelKey)

        <div class="form-group">
            @Html.LabelFor(model => model.InstructionalLevelId, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.TextBoxFor(model => model.InstructionalLevelId, new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.InstructionalLevelId)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.InstructionalLevelName, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.TextBoxFor(model => model.InstructionalLevelName, new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.InstructionalLevelName)
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>
